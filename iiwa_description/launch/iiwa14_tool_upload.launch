<?xml version="1.0"?>
<launch>
  
  <!-- This lauch file just loads the URDF with the given hardware interface and robot name into the ROS Parameter Server -->
  <arg name="hardware_interface" default="EffortJointInterface"/> 
  <arg name="robot_name" default="iiwa"/>
  <!-- <arg name="origin_xyz" default="'0 0 0'"/> 
  <arg name="origin_rpy" default="'0 0 0'"/>-->
  <!-- <arg name="tool_name" default="tool"/>
  <param name="/iiwa/toolName" type="string" value="$(arg tool_name)"/>  -->
  <param name="robot_description"
         command="$(find xacro)/xacro --inorder '$(find iiwa_description)/urdf/iiwa14_rs_tool.urdf.xacro'
          hardware_interface:=$(arg hardware_interface)
          robot_name:=$(arg robot_name)" />
  
</launch>